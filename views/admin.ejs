<script src="/js/material.js"></script>
<script src="/admin/js/upload-material.js"></script>
<script src="/admin/js/issue.js"></script>
<script src="/admin/js/delete-material.js"></script>
<script src="/admin/js/update-material.js"></script>
<script src="/admin/js/materialrequest.js"></script>
<script src="/admin/js/joiningrequest.js"></script>
<script src="/admin/js/update-pass.js"></script>
<script src="/admin/js/main.js"></script>
</body>
</html>